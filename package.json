{
  "name": "mastra-mux-workflow",
  "version": "1.0.0",
  "description": "A comprehensive video processing solution built with Mastra and Mux MCP integration",
  "main": "dist/mastra/index.js",
  "type": "module",
  "scripts": {
    "dev": "NODE_OPTIONS='--import tsx/esm' mastra dev",
    "dev:alt": "tsx --esm src/mastra/index.ts",
    "build": "tsc",
    "start": "node dist/mastra/index.js",
    "test:basic-agent": "tsx src/mastra/scripts/test-basic-agent.ts",
    "test:simple": "tsx src/mastra/scripts/simple-test.ts",
    "basic:interactive": "tsx src/mastra/scripts/basic-interactive.ts",
    "test:tool-names": "tsx src/mastra/scripts/tool-name-test.ts",
    "test:explicit": "tsx src/mastra/scripts/explicit-tool-test.ts",
    "test:constrained": "tsx src/mastra/scripts/constrained-agent.ts",
    "test:debug-explicit": "tsx src/mastra/scripts/debug-explicit-test.ts",
    "test:ollama": "tsx src/mastra/scripts/simple-ollama-test.ts",
    "test:non-streaming": "tsx src/mastra/scripts/non-streaming-test.ts",
    "test:fixed-ollama": "tsx src/mastra/scripts/simple-ollama-test.ts",
    "test:direct-tool": "tsx src/mastra/scripts/direct-tool-only-test.ts",
    "test:minimal-agent": "tsx src/mastra/scripts/minimal-agent-test.ts"
  },
  "dependencies": {
    "@ai-sdk/openai-compatible": "latest",
    "@mastra/core": "latest",
    "@mastra/loggers": "latest",
    "@mastra/mcp": "latest",
    "ai": "latest",
    "dotenv": "latest",
    "mastra": "latest",
    "ollama": "latest",
    "zod": "latest"
  },
  "devDependencies": {
    "@types/node": "latest",
    "tsx": "latest",
    "typescript": "latest"
  },
  "engines": {
    "node": "latest"
  },
  "keywords": [
    "mastra",
    "mux",
    "video",
    "processing",
    "ai",
    "mcp",
    "ollama"
  ],
  "author": "",
  "license": "MIT"
}
