{
  "name": "mastra-mux-workflow",
  "version": "1.0.0",
  "description": "A comprehensive video processing solution built with Mastra and Mux MCP integration",
  "main": "dist/mastra/index.js",
  "type": "module",
  "scripts": {
    "dev": "NODE_OPTIONS='--import tsx/esm' mastra dev",
    "dev:alt": "tsx --esm src/mastra/index.ts",
    "build": "tsc",
    "start": "node dist/mastra/index.js",
    "test:basic-agent": "tsx src/mastra/scripts/test-basic-agent.ts",
    "test:simple": "tsx src/mastra/scripts/simple-test.ts",
    "basic:interactive": "tsx src/mastra/scripts/basic-interactive.ts",
    "test:tool-names": "tsx src/mastra/scripts/tool-name-test.ts",
    "test:explicit": "tsx src/mastra/scripts/explicit-tool-test.ts",
    "test:constrained": "tsx src/mastra/scripts/constrained-agent.ts"
  }

,
  "dependencies": {
    "@ai-sdk/openai-compatible": "^1.0.0",
    "@mastra/core": "latest",
    "@mastra/loggers": "latest",
    "@mastra/mcp": "latest",
    "dotenv": "^16.3.1",
    "mastra": "latest",
    "ollama": "^0.5.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "mastra",
    "mux",
    "video",
    "processing",
    "ai",
    "mcp",
    "ollama"
  ],
  "author": "",
  "license": "MIT"
}
