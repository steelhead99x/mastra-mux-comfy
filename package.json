{
  "name": "mastra-mux-workflow",
  "version": "1.0.0",
  "description": "A comprehensive video processing solution built with Mastra and Mux MCP integration",
  "main": "dist/mastra/index.js",
  "type": "module",
  "scripts": {
    "dev": "NODE_OPTIONS='--import tsx/esm' mastra dev",
    "build": "tsc",
    "start": "node dist/mastra/index.js",
    "test:auth": "tsx src/mastra/scripts/test-mux-auth.ts",
    "test:assets": "tsx src/mastra/scripts/test-list-assets.ts",
    "test:assets:all": "tsx src/mastra/scripts/test-list-assets.ts all",
    "test:vnext": "tsx src/mastra/scripts/ollama-vnext-demo.ts",
    "test:lmstudio": "tsx src/mastra/scripts/test-lmstudio.ts",
    "test:comfyClient": "tsx src/mastra/scripts/comfy-test.ts",
    "agent:anthropic": "tsx src/mastra/agents/anthropic-dynamic-agent.ts",
    "agent:ollama-interactive-data": "tsx src/mastra/agents/interactive-agent-errors.ts",
    "agent:ollama-interactive-analytics": "tsx src/mastra/agents/interactive-agent-analytics.ts",
    "agent:ollama-interactive": "tsx src/mastra/agents/interactive-agent-analytics.ts",
    "test:anthropic": "tsx src/mastra/scripts/interactive-agent-anthropic.ts"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.17",
    "@ai-sdk/openai-compatible": "latest",
    "@mastra/core": "latest",
    "@mastra/libsql": "latest",
    "@mastra/loggers": "latest",
    "@mastra/mcp": "latest",
    "@mastra/memory": "latest",
    "ai": "latest",
    "dotenv": "latest",
    "mastra": "latest",
    "ollama": "latest",
    "zod": "latest"
  },
  "devDependencies": {
    "@types/node": "latest",
    "tsx": "latest",
    "typescript": "latest"
  },
  "engines": {
    "node": ">24.0.0"
  },
  "keywords": [
    "mastra",
    "mux",
    "video",
    "processing",
    "ai",
    "mcp",
    "ollama",
    "anthropic"
  ],
  "author": "",
  "license": "MIT"
}
